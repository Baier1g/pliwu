    0 $accept: program $end

    1 program: ε
    2        | program module

    3 module: funcDefinition
    4       | declaration

    5 declaration: varDeclaration
    6            | statement

    7 varDeclaration: type T_IDENTIFIER T_ASSIGN expression ';'

    8 type: T_INT_TYPE
    9     | T_CHAR_TYPE
   10     | T_BOOL_TYPE
   11     | T_VOID_TYPE

   12 funcDefinition: T_FUNC function

   13 function: T_IDENTIFIER T_LEFT_PAREN parameters T_RIGHT_PAREN T_ARROW type block

   14 parameters: ε
   15           | type T_IDENTIFIER
   16           | parameters T_COMMA type T_IDENTIFIER

   17 args: ε
   18     | assignExpression
   19     | args T_COMMA assignExpression

   20 statement: ifStatement
   21          | returnStatement
   22          | expressionStatement

   23 ifStatement: T_IF T_LEFT_PAREN expression T_RIGHT_PAREN block else

   24 else: ε
   25     | T_ELSE T_LEFT_PAREN expression T_RIGHT_PAREN block

   26 returnStatement: T_RETURN expressionStatement

   27 block: T_LEFT_BRACE blockBody T_RIGHT_BRACE

   28 blockBody: ε
   29          | blockBody declaration

   30 expressionStatement: expression ';'

   31 expression: assignExpression

   32 assignExpression: conditionalExpression
   33                 | T_IDENTIFIER T_ASSIGN expression

   34 conditionalExpression: logicalOR

   35 logicalOR: logicalAND
   36          | logicalOR T_OR logicalAND

   37 logicalAND: equalityExpression
   38           | logicalAND T_AND equalityExpression

   39 equalityExpression: relationalExpression
   40                   | equalityExpression T_EQUALS relationalExpression
   41                   | equalityExpression T_NEQUALS relationalExpression

   42 relationalExpression: additiveExpression
   43                     | relationalExpression '<' additiveExpression
   44                     | relationalExpression '>' additiveExpression
   45                     | relationalExpression T_LESS_EQ additiveExpression
   46                     | relationalExpression T_GREATER_EQ additiveExpression

   47 additiveExpression: multiplicativeExpression
   48                   | additiveExpression '+' multiplicativeExpression
   49                   | additiveExpression '-' multiplicativeExpression

   50 multiplicativeExpression: castExpression
   51                         | multiplicativeExpression '*' castExpression
   52                         | multiplicativeExpression '/' castExpression

   53 castExpression: unaryExpression

   54 unaryExpression: postfixExpression
   55                | '-' postfixExpression
   56                | '!' postfixExpression

   57 postfixExpression: primary
   58                  | postfixExpression T_LEFT_PAREN args T_RIGHT_PAREN

   59 primary: T_INT
   60        | T_CHAR
   61        | T_BOOL
   62        | T_IDENTIFIER
   63        | T_LEFT_PAREN expression T_RIGHT_PAREN


