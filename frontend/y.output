    0 $accept: program $end

    1 program: ε
    2        | program module

    3 module: funcDefinition
    4       | declaration

    5 declaration: varDeclaration
    6            | statement

    7 varDeclaration: type T_IDENTIFIER T_ASSIGN expression ';'

    8 type: T_INT_TYPE
    9     | T_CHAR_TYPE
   10     | T_BOOL_TYPE
   11     | T_VOID_TYPE

   12 funcDefinition: T_FUNC function

   13 function: T_IDENTIFIER T_LEFT_PAREN parameters T_RIGHT_PAREN returnType block

   14 returnType: ε
   15           | T_ARROW type

   16 parameters: ε
   17           | type T_IDENTIFIER
   18           | parameters T_COMMA type T_IDENTIFIER

   19 args: ε
   20     | assignExpression
   21     | args T_COMMA assignExpression

   22 statement: ifStatement
   23          | returnStatement
   24          | expressionStatement

   25 ifStatement: T_IF T_LEFT_PAREN expression T_RIGHT_PAREN block else

   26 else: ε
   27     | T_ELSE block

   28 returnStatement: T_RETURN expression ';'

   29 block: T_LEFT_BRACE blockBody T_RIGHT_BRACE

   30 blockBody: ε
   31          | blockBody declaration

   32 expressionStatement: expression ';'

   33 expression: assignExpression

   34 assignExpression: conditionalExpression
   35                 | T_IDENTIFIER T_ASSIGN expression

   36 conditionalExpression: logicalOR

   37 logicalOR: logicalAND
   38          | logicalOR T_OR logicalAND

   39 logicalAND: equalityExpression
   40           | logicalAND T_AND equalityExpression

   41 equalityExpression: relationalExpression
   42                   | equalityExpression T_EQUALS relationalExpression
   43                   | equalityExpression T_NEQUALS relationalExpression

   44 relationalExpression: additiveExpression
   45                     | relationalExpression '<' additiveExpression
   46                     | relationalExpression '>' additiveExpression
   47                     | relationalExpression T_LESS_EQ additiveExpression
   48                     | relationalExpression T_GREATER_EQ additiveExpression

   49 additiveExpression: multiplicativeExpression
   50                   | additiveExpression '+' multiplicativeExpression
   51                   | additiveExpression '-' multiplicativeExpression

   52 multiplicativeExpression: castExpression
   53                         | multiplicativeExpression '*' castExpression
   54                         | multiplicativeExpression '/' castExpression

   55 castExpression: unaryExpression

   56 unaryExpression: postfixExpression
   57                | '-' postfixExpression
   58                | '!' postfixExpression

   59 postfixExpression: primary
   60                  | postfixExpression T_LEFT_PAREN args T_RIGHT_PAREN

   61 primary: T_INT
   62        | T_CHAR
   63        | T_BOOL
   64        | T_IDENTIFIER
   65        | T_LEFT_PAREN expression T_RIGHT_PAREN


